{
  "name": "jturbide-gulp",
  "version": "1.0.0+0e37a38a",
  "description": "A TypeScript-based Gulp setup class to streamline common tasks like compiling Sass, bundling/minifying JavaScript, optimizing images, processing HTML views, and copying files. The main goal is to reduce repetitive boilerplate and centralize Gulp task configuration.",
  "author": "Julien Turbide",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/jturbide-gulp.cjs.js",
  "module": "dist/jturbide-gulp.esm.js",
  "browser": "dist/jturbide-gulp.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/jturbide-gulp.esm.js",
      "require": "./dist/jturbide-gulp.cjs.js",
      "default": "./dist/jturbide-gulp.umd.js"
    }
  },
  "scripts": {
    "yarn": "yarn",
    "build": "rollup -c",
    "build:publish:push:private": "yarn build && yarn publish:push:private",
    "publish:push:private": "yalc publish --push --private --sig",
    "postinstall": "patch-package"
  },
  "browserslist": [
    "last 2 versions",
    "> 2%"
